Cell["\<\
Nice word query:
- It contains at least three vowels (aeiou only)
- It contains at least one letter that appears twice in a row
- It does not contain the strings ab, cd, pq, or xy\
\>", "Text",
 CellChangeTimes->{{3.6925297411636553`*^9, 3.6925297446378403`*^9}, {
  3.6925297980662527`*^9, 3.6925298066474075`*^9}, {3.6925299254498496`*^9, 
  3.69252992626505*^9}, {3.6925300200710835`*^9, 3.69253002128209*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"NiceQ", "[", "word_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"ch", ",", "wovels", ",", "doubles", ",", "bans"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"ch", "=", 
      RowBox[{"Characters", "@", "word"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"wovels", "=", 
      RowBox[{"Length", "@", 
       RowBox[{"Select", "[", 
        RowBox[{"ch", ",", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"#", "\[Equal]", "\"\<a\>\""}], ")"}], "||", 
           RowBox[{"(", 
            RowBox[{"#", "\[Equal]", "\"\<e\>\""}], ")"}], "||", 
           RowBox[{"(", 
            RowBox[{"#", "\[Equal]", "\"\<i\>\""}], ")"}], "||", 
           RowBox[{"(", 
            RowBox[{"#", "\[Equal]", "\"\<o\>\""}], ")"}], "||", 
           RowBox[{"(", 
            RowBox[{"#", "\[Equal]", "\"\<u\>\""}], ")"}]}], "&"}]}], 
        "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"doubles", "=", 
      RowBox[{"Length", "@", 
       RowBox[{"Select", "[", 
        RowBox[{
         RowBox[{"Partition", "[", 
          RowBox[{"ch", ",", "2", ",", "1"}], "]"}], ",", 
         RowBox[{
          RowBox[{
           RowBox[{
           "#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
           "\[Equal]", 
           RowBox[{
           "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
          "&"}]}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"bans", "=", 
      RowBox[{"Length", "@", 
       RowBox[{"Select", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"StringJoin", "/@", 
           RowBox[{"Partition", "[", 
            RowBox[{"ch", ",", "2", ",", "1"}], "]"}]}], ")"}], ",", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"#", "\[Equal]", "\"\<ab\>\""}], ")"}], "||", 
           RowBox[{"(", 
            RowBox[{"#", "\[Equal]", "\"\<cd\>\""}], ")"}], "||", 
           RowBox[{"(", 
            RowBox[{"#", "\[Equal]", "\"\<pq\>\""}], ")"}], "||", 
           RowBox[{"(", 
            RowBox[{"#", "\[Equal]", "\"\<xy\>\""}], ")"}]}], "&"}]}], 
        "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"wovels", "\[GreaterEqual]", "3"}], ")"}], "&&", 
      RowBox[{"(", 
       RowBox[{"doubles", "\[GreaterEqual]", "1"}], ")"}], "&&", 
      RowBox[{"(", 
       RowBox[{"bans", "\[Equal]", "0"}], ")"}]}]}]}], "\[IndentingNewLine]", 
